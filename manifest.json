{
    "manifest_version": 3,
    "name": "Canvas Tab Manager",
    "description": "A tab manager that links to canvas courses",
    "version": "1.0",
    "icons": {
        "128": "images/logo.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage",
        "favicon"
    ],
    "optional_permissions": [
        "webRequest"
    ],
    "optional_host_permissions": [
        "https://*.instructure.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "action": {}
}